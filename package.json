{
  "name": "react-demo-scrolling-anchor",
  "version": "0.1.0",
  "description": "A demo React.js application for scrolling anchor.",
  "keywords": [
    "mornya",
    "react",
    "vessel",
    "demo",
    "scrolling-anchor"
  ],
  "homepage": "https://mornya.github.io/react-demo-scrolling-anchor",
  "license": "MIT",
  "author": "mornya <mornya78@gmail.com>",
  "private": true,
  "repository": "https://github.com/mornya/react-demo-scrolling-anchor",
  "bugs": "https://github.com/mornya/react-demo-scrolling-anchor/issues",
  "types": "./types/index.d.ts",
  "scripts": {
    "login": "react-vessel login",
    "lint": "react-vessel lint",
    "lint:all": "react-vessel lint all",
    "clean": "react-vessel clean",
    "build": "react-vessel build",
    "start": "react-vessel start",
    "serve": "react-vessel start",
    "dev": "react-vessel dev",
    "test": "react-vessel test",
    "test:watch": "react-vessel test watch",
    "analyze": "react-vessel analyze",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "@mornya/react-helper": "^0.8.1",
    "@mornya/scrolling-anchor-libs": "^1.1.0",
    "immer": "^5.3.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "jest": "^25.1.0",
    "jest-transform-stub": "^2.0.0",
    "prop-types": "^15.7.2",
    "ts-jest": "^25.2.0",
    "tslint": "^5.20.1",
    "tslint-react": "4.1.0",
    "typescript": "^3.7.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": {
    "/mock/": {
      "target": "http://localhost:8080",
      "pathRewrite": {
        "^/mock/": ""
      },
      "secure": false,
      "changeOrigin": false,
      "ws": false,
      "xfwd": true
    }
  },
  "eslintConfig": {
    "root": false,
    "env": {
      "node": true
    }
  },
  "jest": {
    "testURL": "http://test.com/index.html",
    "setupFiles": [
      "<rootDir>/src/test/@setup.ts"
    ],
    "transform": {
      ".([t|j]sx?|mjs)$": "ts-jest",
      ".(bmp|gif|jpe?g|png|tif|webp|svg|mp4|webm|flac|mp3|wav|ogg|aac|eot|[t|o]tf|woff2?)$": "jest-transform-stub",
      ".(s?css|styl(us)?)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testRegex": "\\.test\\.([t|j]sx?|mjs)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "mjs"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "vessel": {
    "minVersion": "3.0.0"
  }
}
